Host rancher-master em-sbg-rancher-srv-5.lan
    HostName em-sbg-rancher-srv-5.lan
    User rancher
    AddKeysToAgent yes
    IdentityFile ~/.ssh/work_id_rsa
    UseKeychain yes

Host rancher-worker-1 em-sbg-rancher-srv-6.lan
    HostName em-sbg-rancher-srv-6.lan
    User rancher
    AddKeysToAgent yes
    IdentityFile ~/.ssh/work_id_rsa
    UseKeychain yes

Host rancher-worker-2 em-sbg-rancher-srv-3.lan
    HostName em-sbg-rancher-srv-3.lan
    User rancher
    AddKeysToAgent yes
    IdentityFile ~/.ssh/work_id_rsa
    UseKeychain yes

Host *
    AddKeysToAgent yes
    UseKeychain yes
    IdentityFile ~/.ssh/id_rsa

